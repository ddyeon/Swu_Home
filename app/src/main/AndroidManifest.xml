<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swu_home">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AddContactActivity"></activity>
        <activity android:name=".activity.AlarmActivity"></activity>
        <activity android:name=".activity.AlertActivity"></activity>
        <activity android:name=".activity.ContactActivity"></activity>
        <activity android:name=".activity.SitListActivity"></activity>
        <activity android:name=".activity.SettingActivity"></activity>
        <activity android:name=".fragment.FragmentSitList"></activity>
        <activity android:name=".fragment.FragmentContact"></activity>


    </application>

</manifest>